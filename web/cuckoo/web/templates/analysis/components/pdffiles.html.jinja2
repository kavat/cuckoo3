{# PDF General Content #}
<div>
  <div>
    {% call ui_box(title="PDF General Content", has_padding=False) %}
      <p class="label no-margin-top">PDF General Content</p>
      <textarea rows="20" cols="140" readonly style="border:none;">
      {% for line in pre.static.pdf.pdf_general_content %}
        {{ line }}
      {% endfor %}
      </textarea>
    {% endcall %}
  </div>
  <div>
    {% call ui_box(title="PE Decoded Content", has_padding=False) %}
      <p class="label no-margin-top">PDF Decoded Content</p>
      <div>{{ pre.static.pdf.pdf_decoded_content }}</div>
    {% endcall %}
  </div>
</div>
